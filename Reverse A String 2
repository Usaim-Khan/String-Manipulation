Module Program
    'Reverse A String
    Sub Main()
        Dim str, newstr As String
        Dim i As Integer
        newstr = ""

        Console.WriteLine("Enter a String")
        str = Console.ReadLine

        For i = 1 To Len(str)
            newstr = Mid(str, i, 1) + newstr
        Next



        Console.WriteLine("Final String: " & newstr)

    End Sub
End Module
